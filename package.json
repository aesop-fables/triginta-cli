{
  "name": "@aesop-fables/triginta-cli",
  "version": "0.4.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/aesop-fables/triginta-cli.git"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "rm -rf ./lib && tsc --p ./tsconfig.json",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "prepublishOnly": "npm run lint && npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "bin": {
    "triginta": "bin/triginta"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@jridgewell/trace-mapping": "^0.3.15",
    "arg": "^5.0.2",
    "chalk": "^5.2.0",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "glob": "^8.1.0",
    "jest": "^29.4.2",
    "string-builder": "^0.1.8",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@aesop-fables/containr": "^0.2.4",
    "@aesop-fables/triginta": "^0.4.0",
    "@babel/core": "^7.11.6",
    "@jest/test-utils": "^0.0.0",
    "@middy/core": "^4.2.7",
    "@jest/types": "29.4.2",
    "@types/babel__core": "^7.1.14",
    "@types/convert-source-map": "^2.0.0",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.0.1",
    "@types/graceful-fs": "^4.1.3",
    "@types/lodash": "^4.14.191",
    "@types/micromatch": "^4.0.1",
    "@types/write-file-atomic": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "babel-plugin-istanbul": "^6.1.1",
    "dedent": "^0.7.0",
    "esbuild": "^0.17.16",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@aesop-fables/containr": "0.2.x",
    "@aesop-fables/triginta": "0.4.x",
    "@middy/core": "4.x"
  },
  "engines": {
    "node": "^14.15.0 || ^16.10.0 || >=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
